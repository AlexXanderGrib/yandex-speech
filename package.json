{
  "name": "yandex-speech-promise",
  "version": "1.3.0",
  "description": "Promise based implementation of Yandex Speech Kit API",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexXanderGrib/yandex-speech-promise.git"
  },
  "author": "AlexXanderGrib <me@alexxgrib.me> (https://alexxgrib.me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlexXanderGrib/yandex-speech-promise/issues"
  },
  "homepage": "https://github.com/AlexXanderGrib/yandex-speech-promise#readme",
  "scripts": {
    "build": "rm -rf dist && tsc --project ./tsconfig.json",
    "prepublishOnly": "npm run build",
    "dev": "tsc -w"
  },
  "files": [
    "dist/",
    "types/",
    "index.d.ts",
    "index.js"
  ],
  "keywords": [
    "yandex",
    "tts",
    "stt",
    "asr",
    "speech",
    "synthesis",
    "recongition",
    "speechkit"
  ],
  "devDependencies": {
    "@types/node": "10",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {},
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "plugin:@typescript-eslint/recommended",
      "prettier",
      "prettier/@typescript-eslint",
      "plugin:import/typescript"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "import/extensions": "off"
    }
  },
  "engines": {
    "node": ">10"
  }
}
